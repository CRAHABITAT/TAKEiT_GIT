        $('#graphicData').html('<IMG src="@Url.Content("~/Content/themes/image/Loader.gif")" alt="">')
        var mapToSend = { "Sku": $('#skuToSend').val() };
        $.ajax({
            type: 'POST',
            url: '@Url.Content("~/Home/EvolutionGraphiquePrix")',
            data: mapToSend,
            success: function (data) {
                $('#graphicData').html(data);
            }
        });

        $('#skuToSend').hide();
        $('#visualisationDetails').show();
        $('#modificationDetails').hide();

        $('#editerProduit').click(function () {
            $('#visualisationDetails').hide();
            $('#modificationDetails').show();
        });

        $('#annulerModif').click(function () {
            $('#visualisationDetails').show();
            $('#modificationDetails').hide();
            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/MenuSkusassocie")',
                data: { "Sku": $('#skuToSend').val() },
                success: function (data) {
                    $('#MenuSkusassocie').html(data);
                }
            });
            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/MenuSkusgroupe")',
                data: { "Sku": $('#skuToSend').val() },
                success: function (data) {
                    $('#MenuSkusgroupe').html(data);
                }
            });
            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/MenuMatiere")',
                data: { "Sku": $('#skuToSend').val() },
                success: function (data) {
                    $('#MenuMatiere').html(data);
                }
            });
            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/MenuCouleur")',
                data: { "Sku": $('#skuToSend').val() },
                success: function (data) {
                    $('#MenuCouleur').html(data);
                }
            });
        });

        $('#saveModif').click(function () {

            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/SaveFiche")',
                data: $('#editForm').serialize(),
                success: function (data) {
                    window.location = 'Fiche?Sku=' + $('#skuToSend').val();
                }
            });

            $('#visualisationDetails').show();
            $('#modificationDetails').hide();
        });




        $('#skuToSend2').hide();
